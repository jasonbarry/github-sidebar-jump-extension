{
	"name": "GitHub Minimap by Netlify",
	"version": "0.0.2",
	"description": "Add a table of contents to your GitHub issues and pull requests.",
	"homepage_url": "https://github.com/netlify/github-minimap-extension",
	"manifest_version": 3,
	"minimum_chrome_version": "100",
	"browser_specific_settings": {
		"gecko": {
			"id": "awesome-extension@notlmn.github.io",
			"strict_min_version": "100.0"
		}
	},
	"icons": {
		"128": "icon.png"
	},
	"permissions": [],
	"host_permissions": ["https://github.com/*"],
	"content_scripts": [
		{
			"matches": ["https://github.com/*"],
			"js": ["content.js"],
			"css": [],
			"run_at": "document_end"
		}
	]
}
